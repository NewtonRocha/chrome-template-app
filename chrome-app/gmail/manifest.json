{
  "name": "Chrome Template App",
  "description": "Extension for use of quick templates emails",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["lib/inboxsdk.js", "js/content.js", "src/iframe.js"],
      "css": [],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "web_accessible_resources": [
    "iframe.html"
  ],
  "manifest_version": 2
}
